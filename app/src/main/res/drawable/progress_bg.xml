<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dp"/>
            <gradient android:startColor="#ff9d9e9d" android:endColor="#ff9d9e9d" android:angle="270" android:centerY="0.75" android:centerColor="#ff9d9e9d"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5dp"/>
                <gradient android:startColor="@color/colorYellowLite" android:endColor="@color/colorYellow" android:angle="180" android:centerY="0.75" android:centerColor="@color/colorYellow"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5dp"/>
                <gradient android:startColor="@color/colorYellowLite" android:endColor="@color/colorYellow" android:angle="180"/>
            </shape>
        </clip>
    </item>
</layer-list>
